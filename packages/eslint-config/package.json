{
	"name": "@kitschpatrol/eslint-config",
	"version": "5.0.0",
	"description": "ESLint configuration for @kitschpatrol/shared-config.",
	"keywords": [
		"shared-config",
		"eslint-config",
		"eslint",
		"cli",
		"kpi",
		"kpi-eslint"
	],
	"bugs": "https://github.com/kitschpatrol/shared-config/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/shared-config/cli.git",
		"directory": "packages/eslint-config"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"kpi-eslint": "bin/cli.js"
	},
	"files": [
		"bin/*",
		"dist/*",
		"init/*"
	],
	"scripts": {
		"build": "tsup --format esm --clean --dts && ../../scripts/build.ts",
		"cli": "node ./bin/cli.js",
		"inspect": "npx @eslint/config-inspector --config ./init/eslint.config.ts",
		"prepublishOnly": "pnpm run build",
		"update-rules": "tsx scripts/presetgen.ts && tsx scripts/typegen.ts && kpi-eslint fix && kpi-prettier fix"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
		"@eslint-react/eslint-plugin": "^1.26.2",
		"@html-eslint/eslint-plugin": "^0.34.0",
		"@html-eslint/parser": "^0.34.0",
		"@pinojs/json-colorizer": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^8.23.0",
		"@typescript-eslint/parser": "^8.23.0",
		"@vitest/eslint-plugin": "^1.1.25",
		"astro-eslint-parser": "^1.2.1",
		"eslint": "^9.19.0",
		"eslint-config-flat-gitignore": "^2.0.0",
		"eslint-flat-config-utils": "^2.0.1",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-mdx": "^3.1.5",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-depend": "^0.12.0",
		"eslint-plugin-html": "^8.1.2",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-jsdoc": "^50.6.3",
		"eslint-plugin-jsonc": "^2.19.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-mdx": "^3.1.5",
		"eslint-plugin-n": "^17.15.1",
		"eslint-plugin-no-only-tests": "^3.3.0",
		"eslint-plugin-package-json": "^0.23.1",
		"eslint-plugin-perfectionist": "^4.8.0",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-svelte": "^2.46.1",
		"eslint-plugin-toml": "^0.12.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"eslint-plugin-yml": "^1.16.0",
		"execa": "^9.5.2",
		"fs-extra": "^11.3.0",
		"globals": "^15.14.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^1.0.0",
		"prettier": "^3.4.2",
		"sort-package-json": "^2.14.0",
		"svelte-eslint-parser": "^0.43.0",
		"toml-eslint-parser": "^0.10.0",
		"yaml-eslint-parser": "^1.2.3"
	},
	"devDependencies": {
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint-plugin-jsx-a11y": "^6.10.0",
		"dot-prop": "^9.0.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-config-xo-typescript": "^7.0.0",
		"eslint-typegen": "^1.0.0",
		"svelte": "^5.19.9",
		"tsup": "^8.3.6"
	},
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=10.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
