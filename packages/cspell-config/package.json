{
	"name": "@kitschpatrol/cspell-config",
	"version": "4.7.12",
	"type": "module",
	"description": "CSpell configuration for @kitschpatrol/shared-config.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/shared-config/cli.git",
		"directory": "packages/cspell-config"
	},
	"bugs": "https://github.com/kitschpatrol/shared-config/issues",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"license": "MIT",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9.0.0"
	},
	"bin": {
		"cspell-config": "bin/cli.js"
	},
	"main": "dist/cspell.config.js",
	"types": "dist/cspell.config.d.ts",
	"files": [
		"bin/*",
		"dictionaries/*",
		"dist/*",
		"init/*"
	],
	"keywords": [
		"shared-config",
		"cspell-config",
		"cspell",
		"cli"
	],
	"scripts": {
		"build": "tsc && ../../scripts/build.ts && pnpm mdat readme",
		"cli": "node ./bin/cli.js",
		"find-duplicates": "cat ./dictionaries/*.txt | sort -f | uniq -i -d",
		"list-words": "cat ./dictionaries/*.txt",
		"prepublishOnly": "pnpm run build",
		"sort-dictionaries": "find ./dictionaries -name '*.txt' -execdir sort -f -o {} {} \\;"
	},
	"peerDependencies": {
		"cspell": "^8.16.1"
	},
	"dependencies": {
		"@cspell/cspell-types": "^8.17.2",
		"@pinojs/json-colorizer": "^4.0.0",
		"cosmiconfig": "^9.0.0",
		"execa": "^9.5.2",
		"fs-extra": "^11.3.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
