{
	"name": "@kitschpatrol/prettier-config",
	"version": "4.7.12",
	"type": "module",
	"description": "Prettier configuration for @kitschpatrol/shared-config.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/shared-config/cli.git",
		"directory": "packages/prettier-config"
	},
	"bugs": "https://github.com/kitschpatrol/shared-config/issues",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"license": "MIT",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9.0.0"
	},
	"bin": {
		"prettier-config": "bin/cli.js"
	},
	"main": "dist/prettier.config.js",
	"types": "dist/prettier.config.d.ts",
	"files": [
		"bin/*",
		"dist/*",
		"init/*"
	],
	"keywords": [
		"shared-config",
		"prettier-config",
		"prettier",
		"cli"
	],
	"scripts": {
		"build": "tsc && ../../scripts/build.ts && pnpm mdat readme",
		"cli": "node ./bin/cli.js",
		"prepublishOnly": "pnpm run build"
	},
	"peerDependencies": {
		"prettier": "^3.4.2"
	},
	"dependencies": {
		"@pinojs/json-colorizer": "^4.0.0",
		"@prettier/plugin-php": "^0.22.4",
		"@prettier/plugin-ruby": "^4.0.4",
		"@prettier/plugin-xml": "^3.4.1",
		"cosmiconfig": "^9.0.0",
		"execa": "^9.5.2",
		"fs-extra": "^11.3.0",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-pkg": "^0.18.1",
		"prettier-plugin-sh": "^0.14.0",
		"prettier-plugin-sql": "^0.18.1",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"prettier-plugin-toml": "^2.0.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
